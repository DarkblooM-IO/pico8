version 42
__lua__
function _update()
	h = stat(93)
	m = stat(94)
	s = stat(95)
	str = (h < 10 and "0" or "")..tostr(h)..":"..(m < 10 and "0" or "")..tostr(m)..":"..(s < 10 and "0" or "")..tostr(s)
end

function _draw()
	cls()
	print(str,50,63)
end
