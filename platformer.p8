-- functions

function hitbox(x,y,w,h)
	return {x=x,y=y,w=w,h=h}
end

function drawhitbox(hitbox,clr,fill)
	local fn = fill and rectfill or rect
	fn(hitbox.x,hitbox.y,hitbox.x+hitbox.w,hitbox.y+hitbox.h,clr or 6)
end

function collision(a,b)
	return not (a.x>b.x+b.w 
	         or a.y>b.y+b.h 
	         or a.x+a.w<b.x 
	         or a.y+a.h<b.y)
end
