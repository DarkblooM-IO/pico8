-- main

function _init()
	player = {
		hb  = hitbox(67,0,7,7),
		vel = {x=0,y=0}
	}
	floor = hitbox(0,124,127,3)
end

function _draw()
 cls()
 spr(1,player.hb.x,player.hb.y)
 drawhitbox(floor)
end
